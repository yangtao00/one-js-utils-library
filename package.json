  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npx rollup -c",
    "test": "jest",
    "docs:dev": "vitepress dev docs",
