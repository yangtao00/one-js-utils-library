  "lint-staged": {
    "**/*.{ts,md}": [
      "eslint --fix .",
      "prettier --write ."
    ]
  },
  "homepage": "https://yangtao00.github.io/one-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtao00/one-utils"
